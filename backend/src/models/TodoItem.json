{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "TodoItem",
  "type": "object",
  "properties": {
    "userId": {
      "type": "string"
    },
    "todoId": {
      "type": "string"
    },
    "createdAt": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "dueDate": {
      "type": "string"
    },
    "done": {
      "type": "boolean"
    },
    "attachmentUrl": {
      "type": "string"
    }
  },
  "required": [
    "todoId",
    "name"
  ],
  "additionalProperties": false
}
